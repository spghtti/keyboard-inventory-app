<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <link rel="stylesheet" href="/stylesheets/reset.css" />
  <link rel="stylesheet" href="/stylesheets/style.css" />
  <link rel="shortcut icon" href="#" />
  <title><%= title %></title>
</head>

<body>
  <%- include('sidebar') -%>
  <div class="wrapper">
    <div>
      <h1><%= title %></h1>
    </div>
    <ul>
      <% if(!keyboardinstance_list){ %>
      <p>No instances available</p>
      <% } else{ %>
      <% keyboardinstance_list.forEach((instance) => { %>
      <li>
        <a href="<%= instance.url %>"><%= instance.keyboard.brand.display_name %> <%= instance.keyboard.name %> </a>: <%= instance.status %>
        <% if (instance.status === 'Sold') { %>
        (<%= instance.date_sold_formatted %>)
        <% } %>
      </li>
      <% }); %> <% } %>
    </ul>
  </div>
</body>

</html>